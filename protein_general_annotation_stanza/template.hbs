<!DOCTYPE html>

<html>
  <head>
    <title>Protein General Annotation</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
    {{download_csv}}
    {{download_json}}
  </head>

  <body>
    {{#if general_annotations}}
      <table class='table'>
        <tbody>
          {{#each general_annotations}}
            <tr>
              <th class='span4'>{{name}}</th>
              <td class='span8'>
                <ul>
                  {{#each messages}}
                    <li>{{this}}</li>
                  {{/each}}
                </ul>
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{else}}
      <div>
        No data
      </div>
    {{/if}}
  </body>
</html>
