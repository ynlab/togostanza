<!DOCTYPE html>

<html>
  <head>
    <title>{{title}}</title>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/css/bootstrap.min.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    <h1>{{title}}</h1>
    <dl class="dl-horizontal">
      <dt>Function</dt>
      <dd>{{function.comment}}</dd>

      <dt>Catalytic activity</dt>
      <dd>{{catalytic_activity.comment}}</dd>

      <dt>Cofactor</dt>
      <dd>{{cofactor.comment}}</dd>

      <dt>Subunit structure</dt>
      <dd>{{subunit_structure.comment}}</dd>

      <dt>Subcellular location</dt>
      <dd>
        <ul>
          {{#each subcellular_location}}
            <li>{{alias}}</li>
          {{/each}}
        </ul>
      </dd>

      <dt>Miscellaneous</dt>
      <dd>
        <ul>
          {{#each miscellaneous}}
            <li>{{comment}}</li>
          {{/each}}
        </ul>
      </dd>

      <dt>Sequence similarities</dt>
      <dd>{{sequence_similarities.comment}}</dd>
    </dl>
  </body>
</html>

