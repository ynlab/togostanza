<!DOCTYPE html>
  <head>
    <title>My Awesome</title>
    <link rel="stylesheet" href="{{css_uri}}" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    {{#if genome_info_list}}
      {{#each genome_info_list}}
        <table class='table'>
          <caption>{{this.first.bioproject_id}}</caption> 
          <tbody>
            <tr>
              <th>Description</th>
              <th>RefSeq</th>
              <th>Type</th>
              <th>Size</th>
              <th>Gene</th>
              <th>tRNA</th>
              <th>rRNA</th>
              <th>Other</th>
            </tr>
            {{#each this}}
              <tr>
                <td>{{desc}}</td>
                <td><a href='{{refseq_link}}' target='_blank'>{{refseq_version}}</a></td>
                <td>{{replicon_type}}</td>
                <td>{{sequence_length}}</td>
                <td>{{gene_cnt}}</td>
                <td>{{trna_cnt}}</td>
                <td>{{rrna_cnt}}</td>
                <td>{{other_cnt}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      {{/each}}
    {{else}}
      No items found
    {{/if}}
  </body>
</html>

