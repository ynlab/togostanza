<!DOCTYPE html>

<html>
  <head>
    <title>MDB Variation</title>
    <link rel="stylesheet" href="{{css_uri}}" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>

    {{adjust_iframe_height_script}}
  </head>

  <body>
    <table>
      <thead>
        <tr>
          <th>gene id</th>
          <th>gene product</th>
          <th>gene symbol</th>
          <th>variation id</th>
          <th>location</th>
          <th>variation type</th>
          <th>ref_allele</th>
          <th>allele</th>
      </thead>

      <tbody>
        {{#each variation}}
          <tr>
            <td class='first-column'><a href="{{gene}}" target='_parent'>{{gene_id}}</a></td>
            <td>{{gene_product}}</td>
            <td>{{gene_symbol}}</td>
            <td><a href="{{valiationid}}" target='_parent'>{{valiation_id}}</a></td>
            <td>{{seqid}}:{{location}}</td>
            <td>{{valiation_type}}</td>
            <td>{{ref_allele}}</td>
            <td>{{allele}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </body>
</html>
