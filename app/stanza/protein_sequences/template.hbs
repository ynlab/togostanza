<!DOCTYPE html>

<html>
  <head>
    <title>Protein Sequences</title>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/css/bootstrap.min.css" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
  </head>

  <body>
    <h1>{{title}}</h1>

    {{#if sequences}}
      {{#each sequences}}
        <table class='table table-bordered'>
          <thead>
            <tr>
              <th>Sequence</th>
              <th>Length</th>
              <th>Mass (Da)</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{protein}}</td>
              <td>{{sequence_length}}</td>
              <td>{{mass}}</td>
            </tr>
          </tbody>
        </table>
        <div>
          Last modified: {{modified}}, Version {{version}} <br />
          Checksum: {{checksum}}
        </div>
        <pre>
          {{value}}
        </pre>
      {{/each}}
    {{/if}}
  </body>
</html>

