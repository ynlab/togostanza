<!DOCTYPE html>

<html>
  <head>
    <title>Organism Phenotype</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
    {{download_csv}}
    {{download_json}}
  </head>

  <body>
    {{#if phenotype_items}}
      <table class='table'>
        <tbody>
          {{#each phenotype_items}}
            <tr>
              <th class='span4'>{{phenotype}}</th>
              <td class='span8'>
                {{value}}{{#if deg_flag}}&deg;C{{/if}}
              </td>
            </td>
          {{/each}}
        </tbody>
      </table>
    {{else}}
      <div>
        No data
      </div>
    {{/if}}
  </body>
</html>

