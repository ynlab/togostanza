<!DOCTYPE html>

<html>
<head>
  <title>Organism List</title>
  {{#each css_uri}}
  <link rel="stylesheet" type="text/css" href="{{this}}" />
  {{/each}}
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
  {{adjust_iframe_height_script}}
</head>

<body>
  <div>
    <table class="table">
      <caption>{{#each general_information}}Applied spices of {{gmo_title}}{{/each}}</caption>
      <thead>
        <tr><th>Organism name</th><th>Taxonomy ID</th></tr>
      </thead>
      <tbody>
        {{#each applied_spices_list}}
        <tr>
          <td><a href="http://togogenome.org/organism/{{taxonomy_id}}" target="_top">{{#if label}}{{label}}{{else}}(No Data){{/if}}</a></td>
          <td><a href="{{tax}}" target="_blank">{{taxonomy_id}}</a></td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</body>
</html>
