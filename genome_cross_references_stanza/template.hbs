<!DOCTYPE html>

<html>
  <head>
    <title>Genome Cross References</title>
    {{#each css_uri}}
    <link rel="stylesheet" href="{{this}}" />
    {{/each}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.min.js"></script>
    {{adjust_iframe_height_script}}
    {{download_csv}}
    {{download_json}}
  </head>

  <body>
    {{#if xrefs}}
      {{#each xrefs}}
        <h3>{{bp}}</h3>
        {{#each data}}
          <table class='table'>
          <caption>{{rs}} : {{desc}}</caption>
            <tbody>
              {{#each xref}}
                <tr>
                  <th>{{xref_db}}</th>
                  <td><a href="{{xref}}">{{xref_id}}</a></td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        {{/each}}
      {{/each}}
    {{else}}
      <div>
        No data
      </div>
    {{/if}}
  </body>
</html>
